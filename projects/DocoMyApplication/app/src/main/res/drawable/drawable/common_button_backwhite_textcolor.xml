<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Tap 押下時-->
    <item
        android:state_enabled="true"
        android:state_pressed="true"
        android:color="@color/common_button_backwhite_textcolor_tap"/>

    <!--Disabled 無効時-->
    <item
        android:state_enabled="false"
        android:color="@color/common_button_backwhite_textcolor_disabled"/>

    <!--Normal 通常時-->
    <!--Visited 訪問済み-->
    <item android:color="@color/common_dark_blue"/>
</selector>